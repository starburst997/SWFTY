{
  "name": "swfty",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "mkdir -p ~/haxe",
    "postinstall": "mkdir -p haxe_libraries; lix download;",
    "haxelib": "./scripts/haxelib.sh",
    "test": "cd samples/cli-exporter && openfl build mac -Dsync -final -v && ./bin/macos/bin/SWFTY.app/Contents/MacOS/SWFTY -r $PWD",
    "mac": "cd samples/cli-exporter && openfl build mac -Dsync -final -v",
    "windows": "cd samples/cli-exporter && openfl build windows -Dsync -final -v"
  },
  "dependencies": {
    "imagemin-cli": "^4.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^6.0.1",
    "lix": "starburst997/lix.client#develop"
  }
}
